{
  "name": "equilibrium.js",
  "version": "0.2.0",
  "description": "Front-end framework",
  "module": "dist/js/equilibrium.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run scss-compile && npm run js-compile",
    "scss-compile": "sass src/scss:dist/css",
    "scss-docs": "sassdoc src/scss --dest docs/scss",
    "scss-watch": "sass --watch src/scss:dist/css",
    "js-compile": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estvrtecky/equilibrium.git"
  },
  "keywords": [
    "scss",
    "js"
  ],
  "author": "Erik Štvrtecký",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/estvrtecky/equilibrium/issues"
  },
  "homepage": "https://github.com/estvrtecky/equilibrium#readme",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "babel-loader": "^9.2.1",
    "sass": "^1.77.3",
    "sassdoc": "^2.7.4",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
